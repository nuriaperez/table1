year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype="manual",
         title = "table1: Builds Table-1 for Social Science Publications in MS-WORD",
         author = person("Steven", "Vannoy"),
         year = year,
         note = note,
         url = "http://CRAN.R-project.org/package=tabel1")
